FRAMEWORK_SEARCH_PATHS = "$(PODS_ROOT)/LocationKit"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/LocationKit" "${PODS_ROOT}/Headers/Public/LocationKit/LocationKit"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/LocationKit" -isystem "${PODS_ROOT}/Headers/Public/LocationKit/LocationKit"
OTHER_LDFLAGS = $(inherited) -ObjC -framework "CoreLocation" -framework "LocationKit" -framework "MapKit" -framework "SystemConfiguration"
PODS_ROOT = ${SRCROOT}/Pods